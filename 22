#include <stdio.h>

int main() {
    int numero;
    printf("Digite um numero de 3 digitos: ");
    scanf("%d", &numero);
    
    int centena = numero / 100;
    int dezena = (numero % 100) / 10;
    int unidade = numero % 10;
    
    int invertido = unidade * 100 + dezena * 10 + centena;
    printf("Numero invertido: %d\n", invertido);
    
    return 0;
}

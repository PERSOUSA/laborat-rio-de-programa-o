#include <stdio.h>

int main() {
    printf("Retangulo:\n");
    printf("*****\n");
    printf("*   *\n");
    printf("*   *\n");
    printf("*****\n\n");
    
    printf("Elipse:\n");
    printf("  ***  \n");
    printf(" *   * \n");
    printf(" *   * \n");
    printf("  ***  \n\n");
    
    printf("Seta:\n");
    printf("  *  \n");
    printf(" *** \n");
    printf("*****\n");
    printf("  *  \n");
    printf("  *  \n\n");
    
    printf("Losango:\n");
    printf("  *  \n");
    printf(" * * \n");
    printf("*   *\n");
    printf(" * * \n");
    printf("  *  \n");
    
    return 0;
}
